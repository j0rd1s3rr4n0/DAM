import java.util.Scanner;
public class Exercici15 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//Demanar un número enter que signifiqui una quantitat d’euros i digui la descomposició en bitllets
		//de 5,10,20,50 i 100.
		
		int billets,eur,cinc_eur;
		cinc_eur = 5;
		Scanner e = new Scanner(System.in);
		
		System.out.print("Inserte EUR: ");
		eur = e.nextDouble();
		
		billets =(int) Math.round((eur/cinc_eur)-(1/2));
		
		System.out.println("Billets de "+cinc_eur+": "+billets);
	}

}
